{
    "name": "njoguamos/laravel-zoho-oauth",
    "description": "Generate Zoho API access and refresh token in Laravel 7.0 and up",
    "keywords": [
        "njoguamos",
        "laravel-zoho-oauth"
    ],
    "homepage": "https://github.com/njoguamos/laravel-zoho-oauth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Njogu Amos",
            "email": "njoguamos@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Njoguamos\\LaravelZohoOauth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Njoguamos\\LaravelZohoOauth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Njoguamos\\LaravelZohoOauth\\LaravelZohoOauthServiceProvider"
            ],
            "aliases": {
                "LaravelZohoOauth": "Njoguamos\\LaravelZohoOauth\\LaravelZohoOauthFacade"
            }
        }
    }
}
